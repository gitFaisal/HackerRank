*-----------------------------------------------*
              Sequence Equation
*-----------------------------------------------*


def permutationEquation(p):
    results = []
    n = len(p)

    for i in range(1 , n+1):
        print("i:",i)
        results.append(p.index(p.index(i)+1) + 1)
        print("p.index(i):",p.index(i))
        print(p.index(p.index(i)+1) + 1)
    return results
